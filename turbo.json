{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "prebuild": {
      "dependsOn": ["^prebuild"]
    },
    "build": {
      "dependsOn": ["^build", "prebuild"],
      "outputs": ["dist/**", "stories.js"]
    },
    "tsc": {
      "dependsOn": ["^tsc", "^build"],
      "outputs": ["dist/**", "tsconfig.tsbuildinfo"]
    },
    "docs": {
      "dependsOn": ["^tsc"],
      "outputs": ["tsdoc.json"]
    }
  }
}
